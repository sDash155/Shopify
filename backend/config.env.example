# Database Configuration
# For Render deployment, use DATABASE_URL from your PostgreSQL service
DATABASE_URL=postgresql://username:password@host:port/database?sslmode=require

# Alternative: Use individual parameters
DB_HOST=localhost
DB_PORT=5432
DB_NAME=shopify_analytics
DB_USER=postgres
DB_PASSWORD=password

# Server Configuration
PORT=5000
NODE_ENV=production

# CORS Configuration
# Update this with your frontend domain
CORS_ORIGIN=https://your-frontend-domain.com
